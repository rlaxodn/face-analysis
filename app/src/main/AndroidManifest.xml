<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.capstonetest1">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/face_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CapStoneTest1">
        <activity android:name=".StartActivity"></activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".UvcActivity"></activity>
        <activity
            android:name=".splash_screen"
            android:label="@string/app_name"
            android:theme="@style/Theme.FaceRecognition.NoActionBar"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PeopleActivity"/>
        <activity android:name=".ExplanationActivity"/>
    </application>

    <uses-permission android:name="android.permission.CAMERA" />

</manifest>